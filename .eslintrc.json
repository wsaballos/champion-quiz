{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "linebreak-style": 0,
        "react/jsx-indent": ["error", 2],
        "indent": ["error", 2]
    }
}